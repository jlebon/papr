<html>
  <head>
    <title>Listing</title>
  </head>
<body>
{% if files|length > 1 %}
  {{ files|length }} entries
{% else %}
  {{ files|length }} entry
{% endif %}
  <ul>
{%- if not at_top %}<li><a href="../index.html">..</a></li>{% endif -%}
{% for name, link in files|dictsort -%}
    <li><a href="{{ link }}">{{ name }}</a></li>
{% endfor %}
  </ul>
  </body>
</html>
